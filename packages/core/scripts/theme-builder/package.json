{
  "name": "@ionic/theme-builder",
  "private": true,
  "version": "0.0.1",
  "description": "Theme Builder",
  "scripts": {
    "build": "stencil build --prerender",
    "build-data": "node scripts/build-data.js",
    "dev": "npm run build-data && sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev"
  },
  "dependencies": {
    "@stencil/core": "next"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2"
  },
  "author": "Ionic Team",
  "license": "MIT"
}
